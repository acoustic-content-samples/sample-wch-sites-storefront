{
  "name": "sample-ui-wch-storefront",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": ">=6.11.4"
  },
  "scripts": {
    "ng": "ng",
    "start": "cross-env NODE_ENV=development webpack-dev-server --port=4200",
    "build": "cross-env NODE_ENV=production webpack",
    "test": "karma start ./karma.conf.js",
    "lint": "ng lint",
    "e2e": "protractor ./protractor.conf.js",
    "create-layout": "echo Pulling the content types. Enter the user password for your WCH tenant: && wchtools pull -tlm -I --dir src/wchLayouts && echo Creating the layout, layout mapping, and the angular component. && ibm-wch-sdk-cli create layout --data src/wchLayouts --src src/app --scss --flat",
    "update-app-module": "echo Pulling the content types. Enter the user password for your WCH tenant: && wchtools pull -tlm -I --dir src/wchLayouts && echo Updating the layout settings for new components. && ibm-wch-sdk-cli create layout --data src/wchLayouts --src src/app --scss --flat",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "deploy": "wchtools push -a -A -f -v --dir dist",
    "build-deploy": "npm run build && npm run deploy",
    "addPrefetch": "ibm-wch-sdk-cli add prefetch --script --data ./temp_data/ --src ./src/ ",
    "createSite": "ibm-wch-sdk-cli create site --name testpage --data ./dist/ ",
    "build-preload": " mkdir -p -m 777 temp_data && echo Pulling the site pages. Enter the user password for your WCH tenant: && wchtools pull -A --dir temp_data && npm run addPrefetch && rm -r temp_data &&  npm run build ",
    "build-preload-windows": " mkdir temp_data && echo Pulling the site pages. Enter the user password for your WCH tenant: && wchtools pull -A --dir temp_data && npm run addPrefetch && rmdir /S /Q temp_data &&  npm run build ",
    "install-layouts-from-zip": "node scripts/layoutsFromZip.js ",
    "install-layouts-from-folder": "node scripts/layoutsFromFolder.js",
    "install-layouts-from-git": "node scripts/layoutFromGit.js",
    "init-site": "node scripts/initSite.js",
    "download-site": "mkdir -p -m 777 content-artifacts && wchtools pull -A -v --dir content-artifacts",
    "download-site-win": "mkdir content-artifacts && wchtools pull -A -v --dir content-artifacts",
    "init-content": "wchtools push -a -A -f -v --dir content-artifacts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ngx-translate/core": "^8.0.0",
    "@ngx-translate/http-loader": "^0.1.0",
    "core-js": "^2.4.1",
    "foundation-sites": "^6.4.1",
    "ibm-wch-sdk-ng": "1.0.508",
    "ng2-logger": "^1.0.3",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.1.0",
    "slick-carousel": "^1.7.1",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.2.4",
    "@angular/compiler-cli": "^4.4.6",
    "@angular/language-service": "^4.0.0",
    "@types/jasmine": "2.5.45",
    "@types/node": "~6.0.60",
    "autoprefixer": "^6.5.3",
    "codelyzer": "~3.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.1",
    "cssnano": "^3.10.0",
    "exports-loader": "^0.6.3",
    "file-loader": "^0.10.1",
    "fs-extra": "^3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "ibm-wch-sdk-cli": "1.0.508",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "json-loader": "^0.5.4",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "less-loader": "^4.0.2",
    "postcss-loader": "^1.3.3",
    "postcss-url": "^5.1.2",
    "protractor": "~5.1.2",
    "raw-loader": "^0.5.1",
    "readline": "^1.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.3",
    "script-loader": "^0.7.0",
    "shelljs": "^0.7.8",
    "source-map-loader": "^0.2.0",
    "stream": "0.0.2",
    "style-loader": "^0.13.1",
    "stylus-loader": "^3.0.1",
    "svg-sprite-loader": "^3.0.5",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3",
    "unzip": "^0.1.11",
    "url-loader": "^0.5.7",
    "wchtools-cli": "^2.0.1",
    "webpack": "~2.4.0",
    "webpack-dev-server": "~2.4.5"
  }
}
